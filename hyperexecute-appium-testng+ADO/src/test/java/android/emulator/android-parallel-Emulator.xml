    </test>

    <test name="AndroidEmulatorTest_2">
        <parameter name="platformVersion" value="15" />
        <parameter name="os" value="Android" />
        <parameter name="deviceName" value="Galaxy S25 Plus" />
        <classes>
            <class name="AndroidEmulator" />
        </classes>
    </test>

      
</suite>
